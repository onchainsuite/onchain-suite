{
  "name": "onchain-suite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "type-check": "tsc --noEmit",
    "commit": "cz",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "bun run lint && bun run build",
    "validate": "bun run type-check && bun run lint && bun run prettier",
    "rename:kebab-files": "find . -name '*.ts' -o -name '*.tsx' | grep -v node_modules | grep -v .next | grep -v .git | while read file; do newname=$(echo \"$file\" | sed 's/\\([A-Z]\\)/-\\L\\1/g' | sed 's/^-//' | sed 's/\\/-/-/g'); [ \"$file\" != \"$newname\" ] && mv \"$file\" \"$newname\" 2>/dev/null || true; done",
    "rename:kebab-dirs": "find . -type d \\( -path './node_modules' -o -path './.next' -o -path './.git' \\) -prune -o -type d -print | grep -E '[A-Z]' | sort -r | while read dir; do newdir=$(echo \"$dir\" | sed 's|/\\([A-Z]\\)|/-\\L\\1|g' | sed 's|^-||' | sed 's|/-|/|g'); [ \"$dir\" != \"$newdir\" ] && mv \"$dir\" \"$newdir\" 2>/dev/null || true; done",
    "rename:kebab-all": "npm run rename:kebab-dirs && npm run rename:kebab-files",
    "imports:check": "eslint . --no-fix --rule 'no-restricted-imports: [error, { patterns: [{ group: [../*], message: Use absolute imports with @/ instead of relative parent imports (../) }] }]'",
    "imports:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss,html}\"",
    "lint:fix": "eslint . --fix",
    "check-all": "npm run lint && npm run format",
    "email:dev": "bunx react-email dev --root src/shared/emails"
  },
  "dependencies": {
    "@arcjet/inspect": "^1.0.0-beta.13",
    "@arcjet/next": "^1.0.0-beta.13",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-neon": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^0.5.7",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.3.28",
    "bowser": "^2.12.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-country-state-city": "^1.1.12",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-phone-number-input": "^3.4.12",
    "react-tsparticles": "^2.12.2",
    "resend": "^6.2.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tsparticles-slim": "^2.12.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^20.19.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
